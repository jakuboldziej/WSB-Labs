{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "testMatch": [
    "**/__tests__/**/*.(js|jsx)",
    "**/*.(test|spec).(js|jsx)"
  ],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  }
}